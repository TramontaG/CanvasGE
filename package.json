{
  "name": "sliver-engine",
  "version": "0.0.1-alpha.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "devDependencies": {
    "typescript": "^5.0.0"
  },
  "scripts": {
    "clean": "bun scripts/clean.js",
    "build": "bun run clean && bunx tsc -p tsconfig.build.json",
    "typecheck": "bunx tsc -p tsconfig.json",
    "prepack": "bun run build",
    "docs:install": "cd website && bun install",
    "docs:start": "cd website && bun docusaurus start --port 3025",
    "docs:build": "cd website && bun docusaurus build",
    "docs:serve": "cd website && bun docusaurus serve --port 3025",
    "docs:clear": "cd website && bun docusaurus clear",
    "docs:version": "cd website && bun docusaurus docs:version"
  }
}
